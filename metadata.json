{
  "name": "puppet-vault",
  "version": "3.0.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Puppet module to manage Vault (https://vaultproject.io)",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-vault.git",
  "project_page": "https://github.com/voxpupuli/puppet-vault",
  "issues_url": "https://github.com/voxpupuli/puppet-vault/issues",
  "tags": [
    "vault",
    "hashicorp",
    "secrets"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.24.0 < 9.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 2.0.0 < 7.0.0"
    },
    {
      "name": "puppet/hashi_stack",
      "version_requirement": ">= 1.0.0 < 3.0.0"
    },
    {
      "name": "puppet/systemd",
      "version_requirement": ">= 3.1.0 < 5.0.0"
    },
    {
      "name": "stm/file_capability",
      "version_requirement": ">= 1.0.1 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7.0"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7.0"
      ]
    },
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2016.03"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ]
}
